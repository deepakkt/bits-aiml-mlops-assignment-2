        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "expr": "sum(increase(http_requests_total[30m]))",
          "refId": "A"
        }
