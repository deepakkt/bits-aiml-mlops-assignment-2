      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        }
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{http_status=~\"4..|5..\"}[5m])) / clamp_min(sum(rate(http_requests_total[5m])), 1e-9)",
          "legendFormat": "error_rate",
          "refId": "A"
        }
      ],
      "title": "Error Rate",
      "type": "timeseries"
    }
